Algoritmo Ejemplo_For_Ejemplo_While_contador
	
	//Ejemplo contador con el For
	Definir contador Como Entero;
	
	Para contador = 0 Hasta 10 Con Paso 2 Hacer
		Escribir  "contador : ", contador;
	Fin Para
	
	Escribir ""
	
	//Ejemplo contador con el Mientras 
	
	contador = 0
	
	Mientras contador <= 10 Hacer
		escribir "contador : " , contador 
		contador = contador + 1;
	FinMientras
FinAlgoritmo
